{
  "plugins": ["unused-imports"],
  "extends": ["next/core-web-vitals", "plugin:react-hooks/recommended"],
  "rules": {
    "no-unused-vars": "off",
    "unused-imports/no-unused-imports": "warn",
    "sort-imports": [
      "warn",
      {
        "ignoreDeclarationSort": true
      }
    ]
  }
}
